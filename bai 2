#include <stdio.h>
#include <math.h>

void calcAverage(float a, float b, float c) {
    float average = (a + b + c) / 3;
    printf("Trung binh cong cua 3 so la: %.4f\n", average);
}

int main() {
    float a, b, c;
    printf("Nhap vao 3 so: ");
    scanf("%f %f %f", &a, &b, &c);
    calcAverage(a, b, c);
    return 0;
}
